{
  "name": "monkeyset",
  "version": "0.0.1",
  "description": "A lightweight and performance based OHLCV data manager",
  "main": "src",
  "engines": {
    "node": ">=12.1.0"
  },
  "scripts": {
    "test": "npx nyc mocha tests/mocha/**/*.js",
    "benchmark:file": "node tests/benchmark/File.js",
    "benchmark:filter": "node tests/benchmark/Filter.js",
    "benchmark:operation": "node tests/benchmark/Operation.js",
    "benchmark:random": "node tests/benchmark/Random.js",
    "benchmark": "node tests/benchmark/File.js && node tests/benchmark/Filter.js && node tests/benchmark/Operation.js && node tests/benchmark/Random.js",
    "coveralls:export": "nyc report --reporter=text-lcov | coveralls",
    "coverage:report": "nyc report --reporter=html",
    "docs:create": "del docs /Q && jsdoc -c docs.json",
    "docs:publish": "npm run docs:create && git add README.md docs && git commit -m \"docs update\" && git push origin development"
  },
  "author": {
    "name": "GiveMeAllYourCats",
    "email": "specamps@gmail.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npx nyc check-coverage"
    }
  },
  "nyc": {
    "branches": 95,
    "lines": 95,
    "functions": 95,
    "statements": 95
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.3",
    "husky": "^2.2.0",
    "jsdoc": "^3.6.1",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-logger": "^1.0.6",
    "moment": "^2.24.0",
    "nyc": "^14.1.0",
    "tui-jsdoc-template": "^1.2.2"
  }
}
