{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "excludePattern": "(^|\\/|\\\\)_",
    "exclude": ["src/libs"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "name": " ",
    "useCollapsibles": true,
    "logo": {
      "url": "https://i.imgur.com/hcLIful.png",
      "width": "250px",
      "height": "120px",
      "link": "index.html"
    }
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/tui-jsdoc-template",
    "readme": "README.md"
  }
}
